<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.vhr.mapper.OplogMapper">

    <resultMap id="BaseResultMap" type="com.sh.vhr.model.Oplog">
            <result column="id" property="id" jdbcType="INTEGER" />
            <result column="addDate" property="adddate" jdbcType="DATE" />
            <result column="operate" property="operate" jdbcType="VARCHAR" />
            <result column="hrid" property="hrid" jdbcType="INTEGER" />
    </resultMap>

    <select id="getAllOplogs" resultMap="BaseResultMap">
        select * from oplog;
    </select>
</mapper>